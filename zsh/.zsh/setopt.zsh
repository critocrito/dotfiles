# see zshoptions(1)

# Changing directories
setopt auto_cd
setopt auto_pushd
setopt cdable_vars
setopt pushd_ignore_dups
setopt pushd_minus

# Expansion and Globbing
setopt no_nomatch

# Input/Output
setopt no_flow_control
setopt interactive_comments

# Job Control
setopt long_list_jobs

# Scripts and Functions
setopt multios

# Shell State
setopt login

# Zle
setopt no_beep
